	keywords = {Adaptation, Climate change, Community scenarios, LU, Mitigation, RCP, SSP, Shared Socioeconomic Pathways, \_GLOBIOM, \_MESSAGE, \_REMIND, \_WITCH, modele, theory},
	pages = {153--168},
}

