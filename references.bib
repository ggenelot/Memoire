	number = {9103},
	urldate = {2023-04-06},
	journal = {The Lancet},
	author = {Wakefield, AJ and Murch, SH and Anthony, A and Linnell, J and Casson, DM and Malik, M and Berelowitz, M and Dhillon, AP and Thomson, MA and Harvey, P and Valentine, A and Davies, SE and Walker-Smith, JA},
	month = feb,
