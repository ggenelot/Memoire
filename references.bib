@article{matambo_establishment_nodate,
	title = {{THE} {ESTABLISHMENT}, {DESIGN} {AND} {GOVERNANCE} {OF} {COMMODITY}-{BASED} {SOVEREIGN} {WEALTH} {FUNDS} {IN} {AFRICA}.},
	abstract = {During the 2008 global economic crisis Sovereign Wealth Funds [SWFs] (state owned investment vehicles that invest in the global financial markets for the benefit of the sponsoring government) saved Wall Street, the centre of global finance, from the brink of collapse. These state-owned investment vehicles injected fresh capital to bail out failing financial institutions such as Citigroup, Merrill Lynch and Morgan Stanley as well as collapsing equity markets. Since then, SWFs have become a dominant feature of the global economy. In an effort to avert negative effects of the natural resource curse such as commodity price volatility more and more African governments are turning to commodity-based SWFs. However, many of these efforts are premature. Many African governments are establishing commodity-based SWFs without taking into cognisance factors that enable the successful operationalisation of this institution (i.e., a prudent natural resource revenue management framework, robust macroeconomic policies and a sound governance framework). Consequently, many SWFs on the African continent are premature funds. The primary objective of this paper is twofold. Firstly, it explains how, when and why commodity-based SWFs should be established in Africa. Secondly, it explains how this institution should be governed.},
	language = {en},
	author = {Matambo, Munashe},
	keywords = {⛔ No DOI found},
}

