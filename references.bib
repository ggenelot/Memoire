	month = jan,
	year = {2015},
	note = {Number: Volume 14 Numéro 3
Publisher: Les éditions en environnements VertigO},
	keywords = {/unread, CIRED, Consommation, changement climatique, modes de vie, prospective, transition, énergie},
}

@article{cointe_organising_2019,
	title = {Organising {Policy}-{Relevant} {Knowledge} for {Climate} {Action}: {Integrated} {Assessment} {Modelling}, the {IPCC}, and the {Emergence} of a {Collective} {Expertise} on {Socioeconomic} {Emission} {Scenarios}},
	volume = {32},
