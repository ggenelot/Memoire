@article{hidair_lespace_2009,
	title = {L’espace urbain cayennais : un champ de construction identitaire},
	copyright = {Les contenus de L’Espace politique sont mis à disposition selon les termes de la Licence Creative Commons Attribution - Pas d’Utilisation Commerciale - Pas de Modification 3.0 France.},
	issn = {1958-5500},
	shorttitle = {L’espace urbain cayennais},
	url = {http://journals.openedition.org/espacepolitique/1039},
	doi = {10.4000/espacepolitique.1039},
	abstract = {Parmi les nombreuses et diverses populations qui vivent en Guyane, les Créoles guyanais constituent un noyau important, en raison de l’histoire, même s’ils sont aujourd’hui une minorité. Repliés sur Cayenne, le groupe socioculturel créole guyanais tente de maintenir sa domination politique et culturelle car il connait une diminution proportionnelle du fait des flux migratoires. De plus, cette diminution est masquée par le fait que ce groupe est largement représenté par les élites politiques locales. Afin de conserver le pouvoir, ils ont recours à deux idéologies : l’idéologie des racines et l’idéologie de l’assimilation de la culture française. Ainsi, l’espace urbain reflète l’identité du groupe mais il est aussi constitutif de cette identité par le pouvoir qu’il concède. Il est le lieu où les Créoles guyanais expriment leur existence et leur volonté de domination. Dans ce champ d’investissement identitaire se dévoile aussi l’appartenance territoriale.},
	language = {fr},
	number = {6},
