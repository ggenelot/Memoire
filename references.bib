	journal = {Cities},
	author = {Mazzeo, Giuseppe},
	month = dec,
	year = {2009},
	keywords = {Historic centre, Metropolitan area, Naples, Socio-economic problems, Urban plans and projects, Urban rehabilitation},
	pages = {363--376},
}

