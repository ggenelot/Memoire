@article{ramirez_elusive_2015,
	title = {The {Elusive} {Inclusive}: {Black} {Food} {Geographies} and {Racialized} {Food} {Spaces}: {Black} {Food} {Geographies} and {Racialized} {Food} {Spaces}},
	volume = {47},
