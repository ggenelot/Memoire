	urldate = {2024-02-23},
	journal = {Research Policy},
	author = {Geels, Frank W.},
	month = dec,
	year = {2002},
	keywords = {/unread, Evolutionary theory, Multi-level analysis, Regime shifts, Sailing ships and steamships, Technological transitions, \_QUEST},
